{
  "expo": {
    "name": "Aham",
    "slug": "Aham",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aham",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#286fb4"
      },
      "package": "ro.aham",
      "splash": {
        "image": "./assets/images/adaptive-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#286fb4"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#286fb4"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}